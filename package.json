{
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "node-gyp build && prebuild --all -r napi --strip",
    "install": "node install.js",
    "fresh":"npm i --no-save node-addon-api node-gyp prebuild mocha; npm run build"
  },
  "devDependencies": {
    "prebuild-install": "^5.2.2"
  },
  "optDependencies": {
    "node-addon-api": "^1.6.2",
    "node-gyp": "^3.8.0",
    "prebuild": "^8.1.2",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "bindings": "^1.3.1"
  },
  "gypfile": true,
  "name": "tagscale",
  "description": "nodeJS key-value-store with secondary indices (tags) based on upscaledb",
  "version": "0.1.2",
  "directories": {
    "test": "test"
  },
  "binary": {
    "napi_versions": [
      1,
      2,
      3
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hakt0r/node-tagscale"
  },
  "keywords": [
    "upscaledb",
    "key",
    "value",
    "db",
    "secondary",
    "index"
  ],
